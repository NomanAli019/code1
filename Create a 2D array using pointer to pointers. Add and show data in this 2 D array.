#include<iostream>
using namespace std;


void main() {
	int** arr;
	arr = new int* [3];
	arr[0] = new int[3];
	arr[1] = new int[3];
	arr[2] = new int[3];
	for (int i = 0; i < 3; i++) {
		for (int a = 0; a < 3; a++) {
			cin >> arr[i][a];
		}
	}
	for (int i = 0; i < 3; i++) {
		for (int a = 0; a < 3; a++) {
			cout<< arr[i][a];
		}
		cout << endl;
	}

	system("pause");
}
